{
  "version": 2,
  "build": [
    {
      "src": "poll/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.8"
      }
    },
    {
      "src": "build_file.sh",
      "use": "@vercel/static-build",
      "config": {
        "distdir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "poll/wsgi.py"
    }
  ]
}
